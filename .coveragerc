[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    */settings/*
    */static/*
    */media/*
    */templates/*
    manage.py
    */tests/*
    */test_*.py
    */__pycache__/*
    */node_modules/*
    .git/*
    */locale/*
    */fixtures/*
    */docs/*
    */scripts/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Havoc Project Coverage Report

[xml]
output = coverage.xml
